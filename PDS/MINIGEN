  SET BDY (M211TAA).
  UCLIN.
  DEL SYSMOD(MINIGEN).
  ENDUCL.
  RESETRC.
  SET BDY (GLOBAL).
  UCLIN.
  DEL SYSMOD(MINIGEN).
  ENDUCL.
  RESETRC.
  SET BDY (GLOBAL).
  RECEIVE SELECT(MINIGEN).
  SET BOUNDARY(M211TAA).
  JCLIN.
  APPLY  SELECT(MINIGEN) REDO BYPASS(ID).
//SMPPTFIN DD  *
